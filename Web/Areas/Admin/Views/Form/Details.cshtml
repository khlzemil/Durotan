
@using Web.Areas.Admin.ViewModels.Form
@model FormDetailsVM


<h3>Id:</h3>
<p>@Model.Form.Id</p>

<h3>FullName:</h3>
<p>@Model.Form.FullName</p>


<h3>Email:</h3>
<p>@Model.Form.Email</p>

<h3>Subject:</h3>
<p>@Model.Form.Subject</p>

<h3>Status:</h3>
<p>@Model.Form.Status</p>

